<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" 
                  android:key="first_preferencescreen">  
                  <!--  -->
                  
  <ListPreference android:key="transmission frequency"
                      android:title="Tracking Frequency"
                      android:entries="@+array/time_frequency"
                      android:entryValues="@+array/time_frequency_values" />
  <com.icecondor.nest.DialogPreference android:key="authentication" 
                      android:title="Tracking Authentication"
                      android:dialogTitle="Authentication"
                      android:dialogMessage="Authentication allows position updates to be sent to a location storage provider. Turn on location reporting from the map screen to begin."
                      android:persistent="false"
                       />
  <CheckBoxPreference android:key="beep on fix"
            		  android:title="Beep on GPS fix" />
  <CheckBoxPreference android:key="start on boot"
            		  android:title="Start on boot" />

  <!-- <CheckBoxPreference android:key="wifi enabled" 
                      android:title="Use WiFi Location" /> -->
</PreferenceScreen>
