<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.icecondor.nest"
      android:versionCode="20091205"
      android:versionName="20091205">
    <uses-sdk android:minSdkVersion="4" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" /> 
    <uses-permission android:name="android.permission.INTERNET" />
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".Start" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="icecondor-android-app"/>
            </intent-filter>
        </activity>
        <activity android:name=".Radar" android:label="Radar">
        </activity>
        <activity android:name=".Settings" android:label="Settings">
        </activity>
        <activity android:name=".GeoRssList" android:label="GeoRssList">
        </activity>
        <activity android:name=".GeoRssDetail" android:label="GeoRssDetail">
        </activity>
        <activity android:name=".ActivityLog" android:label="ActivityLog">
        </activity>
        <service android:name=".Pigeon">
        </service>
        <uses-library android:name="com.google.android.maps" />
    </application>

</manifest>
